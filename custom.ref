	file = {:C$\backslash$:/Users/n9190007/OneDrive - Queensland University of Technology/PhD/Bibliography/pdf/Miann{\'{e}} et al.{\_}2017{\_}Analysing the outcome of CRISPR-aided genome editing in embryos Screening, genotyping and quality control.pdf:pdf},
