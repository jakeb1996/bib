	journal = {Science},
	mendeley-groups = {CRISPR reviews {\&} applications},
	number = {6213},
	pmid = {25430774},
	title = {{The new frontier of genome engineering with CRISPR-Cas9}},
	volume = {346},
	year = {2014}
}

