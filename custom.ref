	mendeley-groups = {CRISPR reviews {\&} applications},
	month = {may},
	pages = {68--76},
	publisher = {Academic Press},
	title = {{Analysing the outcome of CRISPR-aided genome editing in embryos: Screening, genotyping and quality control}},
	url = {https://www-sciencedirect-com.ezp01.library.qut.edu.au/science/article/pii/S1046202316302705?{\_}rdoc=1{\&}{\_}fmt=high{\&}{\_}origin=gateway{\&}{\_}docanchor={\&}md5=b8429449ccfc9c30159a5f9aeaa92ffb},
