file = {:C$\backslash$:/Users/n9190007/OneDrive - Queensland University of Technology/PhD/Bibliography/pdf/Bonomo, Deem{\_}2018{\_}The physicist's guide to one of biotechnology's hottest new topics CRISPR-Cas.pdf:pdf},
issn = {14783975},
journal = {Physical Biology},
keywords = {CRISPR,Cas9,clustered regularly interspaced short palindromic,genome editing},
mendeley-groups = {CRISPR reviews {\&} applications},
number = {4},
pmid = {29543191},
publisher = {IOP Publishing},
title = {{The physicist's guide to one of biotechnology's hottest new topics: CRISPR-Cas}},
volume = {15},
year = {2018}
}
