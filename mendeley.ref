author = {Wu, Xuebing and Scott, David A. and Kriz, Andrea J. and Chiu, Anthony C. and Hsu, Patrick D. and Dadon, Daniel B. and Cheng, Albert W. and Trevino, Alexandro E. and Konermann, Silvana and Chen, Sidi and Jaenisch, Rudolf and Zhang, Feng and Sharp, Phillip A.},
