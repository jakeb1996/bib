title = {{Disabling Cas9 by an anti-CRISPR DNA mimic}},
url = {http://advances.sciencemag.org/lookup/doi/10.1126/sciadv.1701620},
volume = {3},
year = {2017}
}
@article{Tothova2017-dj,
