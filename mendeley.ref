author = {Zetsche, Bernd and Gootenberg, Jonathan S. and Abudayyeh, Omar O. and Slaymaker, Ian M. and Makarova, Kira S. and Essletzbichler, Patrick and Volz, Sara E. and Joung, Julia and {Van Der Oost}, John and Regev, Aviv and Koonin, Eugene V. and Zhang, Feng},
doi = {10.1016/j.cell.2015.09.038},
eprint = {NIHMS150003},
file = {:C$\backslash$:/Users/n9190007/OneDrive - Queensland University of Technology/PhD/Bibliography/pdf/Zetsche et al.{\_}2015{\_}Cpf1 Is a Single RNA-Guided Endonuclease of a Class 2 CRISPR-Cas System.pdf:pdf},
isbn = {6176321972},
issn = {10974172},
journal = {Cell},
mendeley-groups = {CRISPR reviews {\&} applications},
number = {3},
pages = {759--771},
pmid = {26422227},
publisher = {Elsevier Inc.},
title = {{Cpf1 Is a Single RNA-Guided Endonuclease of a Class 2 CRISPR-Cas System}},
url = {http://dx.doi.org/10.1016/j.cell.2015.09.038},
volume = {163},
year = {2015}
