file = {:C$\backslash$:/Users/n9190007/OneDrive - Queensland University of Technology/PhD/Bibliography/pdf/Labuhn et al.{\_}2017{\_}Refined sgRNA efficacy prediction improves large- and small-scale CRISPR–Cas9 applications.pdf:pdf},
