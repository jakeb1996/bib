journal = {Frontiers in Plant Science},
keywords = {East African highland banana,Uganda,biofortification,carotenoids,micronutrient deficiency,pro-vitamin A,staple crop,vitamin A deficiency},
mendeley-groups = {Cavendish Banana},
number = {April},
pages = {558},
pmid = {29755496},
title = {{Banana21: From Gene Discovery to Deregulated Golden Bananas}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/29755496{\%}0Ahttp://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5932193{\%}0Ahttp://journal.frontiersin.org/article/10.3389/fpls.2018.00558/full},
