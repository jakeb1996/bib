file = {:C$\backslash$:/Users/n9190007/OneDrive - Queensland University of Technology/PhD/Bibliography/pdf/Canver et al.{\_}2017{\_}Integrated design, execution, and analysis of arrayed and pooled CRISPR genome editing experiments.pdf:pdf},
issn = {1750-2799},
journal = {Doi.Org},
keywords = {amplicon sequencing,cas9,computational,cpf1,crispor,crispr genome editing,crispr pooled screening,crispresso,docker,effect,lentivirus,locus-specific deep sequencing,off-target,on-target effect,saturating mutagenesis,sequence analysis,sgrna design,software pipeline,targeted sequencing,tools},
mendeley-groups = {CRISPR reviews {\&} applications},
number = {5},
