@article{Lei2016,
abstract = {Motivation: Clustered regularly interspaced short palindromic repeats and associated proteins (CRISPR-Cas) allows more specific and efficient gene editing than all previous genetic engineering systems. These exciting discoveries stem from the finding of the CRISPR system being an adaptive immune system that protects the prokaryotes against exogenous genetic elements such as phages. Despite the exciting discoveries, almost all knowledge about CRISPRs is based only on microorganisms that can be isolated, cultured and sequenced in labs. However, about 95{\%} of bacterial species cannot be cultured in labs. The fast accumulation of metagenomic data, which contains DNA sequences of microbial species from natural samples, provides a unique opportunity for CRISPR annotation in uncultivable microbial species. However, the large amount of data, heterogeneous coverage and shared leader sequences of some CRISPRs pose challenges for identifying CRISPRs efficiently in metagenomic data.$\backslash$r$\backslash$n$\backslash$r$\backslash$nResults: In this study, we developed a CRISPR finding tool for metagenomic data without relying on generic assembly, which is error-prone and computationally expensive for complex data. Our tool can run on commonly available machines in small labs. It employs properties of CRISPRs to decompose generic assembly into local assembly. We tested it on both mock and real metagenomic data and benchmarked the performance with state-of-the-art tools.$\backslash$r$\backslash$n$\backslash$r$\backslash$nAvailability and Implementation: The source code and the documentation of metaCRISPR is available at https://github.com/hangelwen/metaCRISPR.},
author = {Lei, Jikai and Sun, Yanni},
doi = {10.1093/bioinformatics/btw456},
file = {:C$\backslash$:/Users/n9190007/OneDrive - Queensland University of Technology/PhD/Bibliography/pdf/Lei, Sun{\_}2016{\_}Assemble CRISPRs from metagenomic sequencing data.pdf:pdf},
issn = {14602059},
journal = {Bioinformatics},
mendeley-groups = {Software applications/todo},
number = {17},
pages = {i520--i528},
pmid = {27587670},
title = {{Assemble CRISPRs from metagenomic sequencing data}},
volume = {32},
year = {2016}
}
