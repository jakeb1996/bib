}
@article{Nunez2014,
abstract = {The initial stage of CRISPR-Cas immunity involves the integration of foreign DNA spacer segments into the host genomic CRISPR locus. The nucleases Cas1 and Cas2 are the only proteins conserved among all CRISPR-Cas systems, yet the molecular functions of these proteins during immunity are unknown. Here we show that Cas1 and Cas2 from Escherichia coli form a stable complex that is essential for spacer acquisition and determine the 2.3-{\AA}-resolution crystal structure of the Cas1-Cas2 complex. Mutations that perturb Cas1-Cas2 complex formation disrupt CRISPR DNA recognition and spacer acquisition in vivo. Active site mutants of Cas2, unlike those of Cas1, can still acquire new spacers, thus indicating a nonenzymatic role of Cas2 during immunity. These results reveal the universal roles of Cas1 and Cas2 and suggest a mechanism by which Cas1-Cas2 complexes specify sites of CRISPR spacer integration.},
author = {Nu{\~{n}}ez, James K. and Kranzusch, Philip J. and Noeske, Jonas and Wright, Addison V. and Davies, Christopher W. and Doudna, Jennifer A.},
doi = {10.1038/nsmb.2820},
file = {:C$\backslash$:/Users/n9190007/OneDrive - Queensland University of Technology/PhD/Bibliography/pdf/Nu{\~{n}}ez et al.{\_}2014{\_}Cas1-Cas2 complex formation mediates spacer acquisition during CRISPR-Cas adaptive immunity.pdf:pdf},
isbn = {1545-9985},
issn = {15459985},
journal = {Nature Structural and Molecular Biology},
mendeley-groups = {CRISPR reviews {\&} applications},
number = {6},
pages = {528--534},
pmid = {24793649},
publisher = {Nature Publishing Group},
title = {{Cas1-Cas2 complex formation mediates spacer acquisition during CRISPR-Cas adaptive immunity}},
url = {http://dx.doi.org/10.1038/nsmb.2820},
volume = {21},
year = {2014}
}
