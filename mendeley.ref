journal = {BMC Bioinformatics},
mendeley-groups = {Software applications/todo},
pages = {1--6},
pmid = {17239253},
title = {{PILER-CR: Fast and accurate identification of CRISPR repeats}},
volume = {8},
year = {2007}
}
