journal = {Bioengineered},
keywords = {C2c2,CRISPR-Cas9,Cpf1,Off-target effect,genome engineering,protospacer adjacent motif},
mendeley-groups = {CRISPR reviews {\&} applications},
number = {3},
pages = {265--273},
pmid = {28140746},
title = {{Cas9, Cpf1 and C2c1/2/3―What's next?}},
volume = {8},
year = {2017}
