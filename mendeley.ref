file = {:C$\backslash$:/Users/n9190007/OneDrive - Queensland University of Technology/PhD/Bibliography/pdf/Fu et al.{\_}2014{\_}Improving CRISPR-Cas nuclease specificity using truncated guide RNAs.pdf:pdf},
isbn = {1546-1696 (Electronic)$\backslash$r1087-0156 (Linking)},
issn = {15461696},
journal = {Nature Biotechnology},
mendeley-groups = {CRISPR reviews {\&} applications},
number = {3},
pages = {279--284},
pmid = {24463574},
title = {{Improving CRISPR-Cas nuclease specificity using truncated guide RNAs}},
volume = {32},
year = {2014}
}
