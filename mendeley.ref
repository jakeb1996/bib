file = {:C$\backslash$:/Users/n9190007/OneDrive - Queensland University of Technology/PhD/Bibliography/pdf/Ran et al.{\_}2013{\_}Double nicking by RNA-guided CRISPR cas9 for enhanced genome editing specificity.pdf:pdf},
isbn = {1097-4172 (Electronic)$\backslash$r0092-8674 (Linking)},
